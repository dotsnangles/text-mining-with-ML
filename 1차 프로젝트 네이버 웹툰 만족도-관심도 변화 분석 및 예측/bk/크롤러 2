emp = 'https://apis.naver.com/commentBox/cbox/web_naver_list_jsonp.json?'\n","    qs = f'ticket=comic&templateId=webtoon&pool=cbox3&_cv=20220331145806&_callback=jQuery11240783762017356769_1649067560893&lang=ko&country=KR&objectId={Id}_1&categoryId=&pageSize=15&indexSize=10&groupId=&listType=OBJECT&pageType=default&page=1&initialize=true&userType=&useAltSort=true&replyPageSize=10&_=1649067560894'\n","    # 쿼리스트링을 딕셔너리로 파싱\n","    params = dict(parse.parse_qsl(qs))\n","    params['_callback'] = ''\n","    params['objectId'] = f'{Id}_{i}' # 회차 수정\n","    # 제이슨 페이지 로드 및 딕셔너리로 파싱\n","    json_page_raw = requests.get(json_url_temp, headers=headers, params=params)\n","    json_page = json_page_raw.text[10:-2]\n","    # 댓글 페이지 딕셔너리\n","    comment_dic = json.loads(json_page)\n","    # 전체 댓글 수\n","    commentCount = comment_dic['result']['count']['comment']\n","    data['전체댓글수'].append(commentCount) # 회차별 전체 댓글 수 data에 추가\n","    # 베댓 좋아요 싫어요 각각의 총합\n","    sympathyTotalCount = 0\n","    antipathyTotalCount = 0\n","    commentListLength = len(comment_dic['result']['commentList'])\n","    for i in range(commentListLength):\n","      sympathyTotalCount += comment_dic['result']['commentList'][i]['sympathyCount']\n","      antipathyTotalCount += comment_dic['result']['commentList'][i]['antipathyCount']\n","    data['베댓공감수'].append(sympathyTotalCount) # 회차별 베댓 좋아요 총합 data에 추가\n","    data['베댓비공감수'].append(antipathyTotalCount) # 회차별 베댓 싫어요 총합 data에 추가\n","\n","\n","  # 회차별 평점 / 회차별 평점 기여자 수 / 회차별 좋아요 수 크롤링\n","\n","  for i in tqdm(range(1, newestEpisodeNum+1)):\n","    # 공용 헤더\n","    headers = {\n","        'referer': f'https://comic.naver.com/comment/comment?titleId={Id}&no={i}', # 회차 수정\n","        'user-agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/99.0.4844.84 Safari/537.36',\n","        'cookie': 'nx_ssl=2; increase=; BMR=s=1649059583690&r=https://m.blog.naver.com/pjt3591oo/220962168316&r2=; nid_inf=2063756930; NID_JKL=kJ2Atuj76y5t//wcMaZ0PadPT+QuDDPnadI9+s6XRkg=; page_uid=hC0Nnsprvh8ss7gWQ0GssssstLw-422217; JSESSIONID=98509AB7DA27050EEB939E8EF0830FD2',\n","    }\n","\n","    # 회차 좋아요 수 크롤링을 위한 제이슨 페이지 쿼리 스트링\n","    json_url_temp = 'https://comic.like.naver.com/likeIt/likeItContent.jsonp?'\n","    qs = f'_callback=window.__jindo2_callback._6524&serviceId=COMIC&displayId=COMIC&contentsId={Id}_{i}&lang=ko&viewType=like'\n","    # 쿼리스트링을 딕셔너리로 파싱\n","    params = dict(parse.parse_qsl(qs))\n","    params['_callback'] = ''\n","    # 제이슨 페이지 로드 및 딕셔너리로 파싱\n","    json_page_raw = requests.get(json_url_temp, headers=headers, params=params)\n","    json_page = json_page_raw.text\n","    _dic = json.loads(json_page)\n","    data['좋아요수'].append(_dic['result']['likeItContent']['likeItCount']) # 회차별 좋아요 수 data에 추가\n","    # 회차별 페이지 로드\n","    url = f'https://comic.naver.com/webtoon/detail?titleId={Id}&no={i}&weekday=wed'\n","    page = requests.get(url, headers=headers)\n","    soup = BeautifulSoup(page.content, 'lxml')\n","    data['평점'].append(soup.select('#topPointTotalNumber > strong')[0].text) # 회차별 평점 data에 추가\n","    data['평점기여자수'].append(soup.select('#topTotalStarPoint > span.pointTotalPerson > em')[0].text) # 회차별 평점 기여자 수 data에 추가\n","\n","  return data"],"metadata":{"id":"t6WpA8tiobKa","executionInfo":{"status":"ok","timestamp":1649227936548,"user_tz":-540,"elapsed":10,"user":{"displayName":"Jeonghyeon Park","userId":"12513544746873038725"}}},"execution_count":2,"outputs":[]},{"cell_type":"code","source":["data = toonInfoCrawler(659934, 202)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"3fK4CroZkx1J","outputId":"445a02c9-ab52-4b54-daca-423086ec4c89","executionInfo":{"status":"ok","timestamp":1649231492211,"user_tz":-540,"elapsed":230744,"user":{"displayName":"Jeonghyeon Park","userId":"12513544746873038725"}}},"execution_count":9,"outputs":[{"output_type":"stream","name":"stderr","text":["100%|██████████| 202/202 [01:34<00:00,  2.14it/s]\n","100%|██████████| 202/202 